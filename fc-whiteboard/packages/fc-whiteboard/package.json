{
  "author": "wx-chevalier@github",
  "description": "fc-whiteboard",
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "name": "fc-whiteboard",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/wx-chevalier/fc-components"
  },
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run copy && npm run build:umd",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:umd": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.umd.js",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 './src/**/*.(less|svg)' dist/cjs/",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "pub": "npm run clean && npm run build && npm publish",
    "start": "npm run dev"
  },
  "dependencies": {
    "eventemitter3": "^3.1.0",
    "fc-hotkeys": "^0.0.1-alpha.1",
    "interactjs": "^1.4.0-rc.13",
    "lodash": "^4.17.11",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "siema": "^1.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/eventemitter3": "^2.0.2",
    "@types/jest": "23.3.12",
    "@types/lodash": "^4.14.126",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/siema": "^1.4.3",
    "@types/uuid": "^3.4.4",
    "autoprefixer": "9.4.4",
    "awesome-typescript-loader": "^5.2.1",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^4.6.0",
    "copyfiles": "^2.1.0",
    "css-loader": "2.1.0",
    "enzyme": "^3.8.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "json-server": "0.14.2",
    "lerna": "^3.8.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.6.3",
    "react-router-dom": "^4.3.1",
    "resolve-url-loader": "3.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.2.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "^1.1.2",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.28.3",
    "webpack-cli": "3.2.1",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "files": [
    "dist/"
  ]
}
